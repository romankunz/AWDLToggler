<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Enable App Sandbox (if your XPC Service is sandboxed) -->
    <key>com.apple.security.app-sandbox</key>
    <true/>
    <!-- Allow the service to create network connections as a client -->
    <key>com.apple.security.network.client</key>
    <true/>
    <!-- Allow the service to look up registered Mach services -->
    <key>com.apple.security.temporary-exception.mach-lookup.global-name</key>
    <array>
        <string>com.rmak.AWDLHelperService</string>
    </array>
    <!-- Specify any additional entitlements below as needed -->
</dict>
</plist>
